package Core.java;

public class Abstraction execution {

	public static void main(String[] args) {

		// class name + reference variable = new + constructor name

		Bmw obj = new Bmw();

		Bmw obj1 = new Bmw();

		System.out.println(obj.year);

		System.out.println(obj1.model);

		System.out.println(Bmw.door);

	}
}